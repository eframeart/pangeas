<!-- Facebook script -->
<script>
var bindFacebookEvents, defer, dispatch, handleEvent, initializeFacebookSDK;
var loadFacebookSDK, loaded, restoreFacebookRoot, saveFacebookRoot;

({defer, dispatch} = Turbolinks);

loaded = false;

$(function() {
  loadFacebookSDK();
  if (!window.fbEventsBound) {
    return bindFacebookEvents();
  }
});

handleEvent = function(eventName, handler) {
  return document.addEventListener(eventName, handler, false);
};

handleEvent("DOMContentLoaded", function() {
  return defer(function() {
    return loaded = true;
  });
});

handleEvent("turbolinks:load", function() {
  if (loaded) {
    return dispatch("page:load");
  }
});

bindFacebookEvents = function() {
  $(document)
    .on('turbolinks:request-start', saveFacebookRoot)
    .on('turbolinks:load', restoreFacebookRoot)
    .on('page:load', function() {
      return typeof FB !== "undefined" && FB !== null ? FB.XFBML.parse() : void 0;
  });
  return this.fbEventsBound = true;
};

saveFacebookRoot = function() {
  if ($('#fb-root').length) {
    return this.fbRoot = $('#fb-root').detach();
  }
};

restoreFacebookRoot = function() {
  if (this.fbRoot != null) {
    if ($('#fb-root').length) {
      return $('#fb-root').replaceWith(this.fbRoot);
    } else {
      return $('body').append(this.fbRoot);
    }
  }
};

loadFacebookSDK = function() {
  window.fbAsyncInit = initializeFacebookSDK;
  return $.getScript("//connect.facebook.net/pt_BR/all.js");
};

initializeFacebookSDK = function() {
  return FB.init({
    appId: '<%= ENV["FACEBOOK_APP_ID"] %>',
    status: true,
    cookie: true,
    version: 'v2.8',
    xfbml: true
  });
};
</script>
<!-- DO NOT MODIFY -->
<!-- End Facebook script -->
